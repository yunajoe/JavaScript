function findIndex(element, k){
    if(element === k ) return true;
}
    // for(let i=0; i < myArr.length; i++){
    //     console.log(i, myArr[i])

function solution(num, k) {
    let answer;
    var myArr = String(num).split('').map(
    (num) => {
        return Number(num)
    })      
    let indexNum = myArr.indexOf(k)
    if(indexNum !== -1){
        answer = indexNum + 1
    }else{
        answer = -1
    }
    return answer 
    }

    
