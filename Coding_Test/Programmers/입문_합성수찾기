

function get_divisor_length(n){
    let divisor_arr = new Array()
    for(let i =1; i<=n/2; i++) {
        if( n % i === 0){
            divisor_arr.push(i) 
        }
    }
    divisor_arr.splice(divisor_arr.length, 0, n)
    console.log(divisor_arr.length)      
}  

// 4부터 n까지의 수 각각의, 약수의 갯수가 나온다
function solution(n) {
    for(let i=4; i<=n; i++){         
        get_divisor_length(i)
    }    
}
