
function solution(my_string) {
    var answer = [];      
    const arr = [...my_string];
    arr.forEach(function(ele){
        ele = parseInt(ele)
        if(isNaN(ele) == false){
            answer.push(ele)
        }
    })
    return answer.sort()
 
}


// 다른사람풀이 

function solution(my_string) {
    var answer = [];
    words = [...my_string]
    // map(ele => ele*1) 했을 때 숫자가 아닌 경우는 NaN으로 나타남
    return words.filter(ele => !isNaN(ele)).map(ele => ele*1).sort((a,b) => a-b)
}
