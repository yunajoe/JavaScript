function solution(n, numlist) {
    var idx = 0; 
    while( idx < numlist.length){
        if(numlist[idx] % n !== 0){
            numlist.splice(idx,1); 
        }else{
            ++idx;  
        }  
    }
    return numlist;
}     


# N의 배수 고르기 

const solution = (n, numlist) => numlist.filter(a => a%n ===0)
