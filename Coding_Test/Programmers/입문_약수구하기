# 나의 풀이 
function solution(n) {
    var answer = [];
    for(let i=1; i<= (n/2) ; i++){
        if(n % i === 0){
            answer.push(i)
        }
    }
    answer.splice(answer.length, 0, n)
    return answer
      
}


# 다른 사람 풀이  
function solution(n) {
    var answer = [];
    for(let i=1; i<=n/2; i++){
        if(n % i === 0) answer.push(i); 
    }
    return [...answer, n];
}
